{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Electron: Main",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/electron/cli.js",
      "args": ["."],
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["--inspect=5858"],
      "console": "integratedTerminal",
      "outputCapture": "std",
      "autoAttachChildProcesses": true
    }
  ]
}
